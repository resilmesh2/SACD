{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "./",
  "projects": {
    "sentinel-layout": {
      "projectType": "library",
      "root": "sentinel-layout",
      "sourceRoot": "sentinel-layout",
      "prefix": "sentinel",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "sentinel-layout/tsconfig.lib.json",
            "project": "sentinel-layout/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "sentinel-layout/tsconfig.lib.prod.json"
            },
            "development": {}
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "codeCoverage": true,
            "main": "sentinel-layout/test.ts",
            "tsConfig": "sentinel-layout/tsconfig.lib.spec.json",
            "karmaConfig": "sentinel-layout/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "sentinel-layout/**/*.ts",
              "sentinel-layout/**/*component.html"
            ],
            "maxWarnings": 0
          }
        }
      }
    },
    "resilmesh-dashboard": {
      "projectType": "application",
      "schematics": {},
      "root": "resilmesh-dashboard",
      "sourceRoot": "resilmesh-dashboard/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "dist/resilmesh-dashboard"
            },
            "index": "resilmesh-dashboard/src/index.html",
            "polyfills": [
              "resilmesh-dashboard/src/polyfills.ts"
            ],
            "tsConfig": "resilmesh-dashboard/tsconfig.app.json",
            "assets": [
              "resilmesh-dashboard/src/favicon.ico",
              "resilmesh-dashboard/src/assets"
            ],
            "styles": [
              "resilmesh-dashboard/src/theme.scss",
              "resilmesh-dashboard/src/styles.scss",

            ],
            "scripts": [],
            "extractLicenses": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "browser": "resilmesh-dashboard/src/main.ts"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "resilmesh-dashboard/src/environments/environment.ts",
                  "with": "resilmesh-dashboard/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "development": {}
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "resilmesh-dashboard:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "resilmesh-dashboard:build:production"
            },
            "development": {
              "buildTarget": "resilmesh-dashboard:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "resilmesh-dashboard:build"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "resilmesh-dashboard/**/*.ts",
              "resilmesh-dashboard/**/*component.html"
            ],
            "maxWarnings": 0
          }
        },
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "watch": false,
            "exit": true,
            "cypressConfig": "resilmesh-dashboard/e2e/cypress.json",
            "tsConfig": "resilmesh-dashboard/e2e/tsconfig.e2e.json",
            "devServerTarget": "resilmesh-dashboard:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "resilmesh-dashboard:serve:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
