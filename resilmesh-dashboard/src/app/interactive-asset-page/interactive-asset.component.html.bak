<div class="search-filter">
  <mat-form-field appearance="outline" class="search-input">
    <mat-label>Search Supernet Node</mat-label>
    <input matInput placeholder="Enter a subnet range" [(ngModel)]="subnetSearch" />
    <mat-hint>Only search by Subnet Node range is supported</mat-hint>
  </mat-form-field>
  <button
    [disabled]="!subnetSearch"
    mat-raised-button
    color="primary"
    (click)="loadCytoscapeData()"
    class="search-button"
  >
    <mat-icon>search</mat-icon>
    Search
  </button>
</div>

<div class="visualisation-container">
  <div
    id="cy"
    *ngIf="graphData?.length; else noData"
    class="cy-container"
  ></div>
  <ng-template #noData>
    <div class="no-data-message">There is no assets being displayed.</div>
  </ng-template>
</div>

